{
  "name": "vue-cli-3-base-template",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prod": "npm run build; export NODE_ENV=production; node server/server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "moment": "^2.22.2",
    "numeral": "^2.0.6",
    "stats-webpack-plugin": "^0.7.0",
    "sw-precache-webpack-plugin": "^0.11.5",
    "vue": "^2.5.17",
    "vue-i18n": "^8.1.0",
    "vue-meta": "^1.5.4",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.1",
    "@vue/cli-plugin-eslint": "^3.0.1",
    "@vue/cli-service": "^3.0.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.0.1",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "comma-dangle": 0,
      "no-console": 0
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
